@model PagedList.IPagedList<BoldRealties.Models.PropertiesRS>
@using PagedList.Mvc; 

@{
    ViewData["Title"] = "Home Page";
}
                <div class="container col-xxl-8 px-4 py-5">
                <div class="row flex-lg-row-reverse align-items-center g-5 py-5">
                    <div class="carousel flex-row justify-content-start w-50">
                        <div id="carouselExampleInterval" class="carousel slide" data-ride="carousel">
                            <div class="carousel-inner">
                                <div class="carousel-item active" data-interval="10000">
                                    <img src="/images/tenancy/image.jpg"  class="d-block w-100" alt="...">
                                </div>
                                <div class="carousel-item" data-interval="2000">
                                    <img src="/images/tenancy/image.jpg"   class="d-block w-100" alt="...">
                                </div>
                                <div class="carousel-item" data-interval="2000">
                                    <img src="/images/tenancy/image.jpg"  class="d-block w-100" alt="...">
                                </div>
                                <div class="carousel-item" data-interval="2000">
                                    <img src="/images/tenancy/image.jpg"  class="d-block w-100" alt="...">
                                </div>
                                <div class="carousel-item" data-interval="2000">
                                    <img src="/images/tenancy/image.jpg"  class="d-block w-100" alt="...">
                                </div>
                                <div class="carousel-item" data-interval="2000">
                                    <img src="/images/tenancy/image.jpg"  class="d-block w-100" alt="...">
                                </div>
                                <div class="carousel-item">
                                    <img src="/images/tenancy/image.jpg"  class="d-block w-100" alt="...">
                                </div>
                                <div class="carousel-item" data-interval="2000">
                                    <img src="/images/tenancy/image.jpg" class="d-block w-100" alt="...">
                                </div>
                            </div>
                            <a class="carousel-control-prev" href="#carouselExampleInterval" role="button" data-slide="prev">
                                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                <span class="sr-only">Previous</span>
                            </a>
                            <a class="carousel-control-next" href="#carouselExampleInterval" role="button" data-slide="next">
                                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                <span class="sr-only">Next</span>
                            </a>
                        </div>
                    </div>
                      <div class="row flex-row justify-content-end w-50">
               <h3 class="display-5 fw-bold">Search Property</h3>
                    @using (Html.BeginForm("Index", "Home", FormMethod.Get))
                        {
                            <div class="card-row align-content-center">
                                Find by address: @Html.TextBox("SearchString", ViewBag.CurrentFilter as string )   
                                 <input type="submit" value="Search" class="btn-sm btn-dark" />
                             </div>
                        }
                       
                  <h5 class="display-6 fw-bold p-6">Free valuation for your property today</h5>
                  <p class="align-items-center">
                      Get free valuation report for your property. No obligations, no contracts, just the accurate price for your property!
                  </p>
                  <button class="btn-sm btn-dark">More details</button>
               </div>
                </div>
                </div>           
        <div class="row pb-3 backgroundWhite">
   @foreach (var properties in Model)
        {
        <div class="col-lg-3 col-md-6">
            <div class="row p-2">
                <div class="col-12  p-1" style="border:1px solid #008cba; border-radius: 5px;">
                    <div class="card" style="border:0px;">
                        <img src="@properties.imagePath" class="card-img-top rounded" />
                        <div class="pl-1">
                            <p class="card-title h5"><b style="color:#2c3e50">@properties.propertyAddress</b></p>
                        </div>
                        <div style="padding-left:5px;">
                            <p>Rent Price: <b class="">$@properties.marketPrice.ToString("0.00")</b></p>
                        </div>
                        <div style="padding-left:5px;">
                            <p style="color:black">Specification: <b class="">@properties.estateDescription</b></p>
                        </div>
                    </div>
                    <div>
                        <a asp-action="Details" class="btn-sm btn-dark form-control"  asp-route-id="@properties.ID">Details</a>
                    </div>
                </div>
            </div>
        </div>
        }
                </div>
