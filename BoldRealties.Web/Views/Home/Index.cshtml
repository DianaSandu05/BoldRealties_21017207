@model IEnumerable<BoldRealties.Models.PropertiesRS>

@{
    ViewData["Title"] = "Home Page";
}
         
                <div class="main">
                    <div class="carousel">
                        <div id="carouselExampleInterval" class="carousel slide" data-ride="carousel">
                            <div class="carousel-inner">
                                <div class="carousel-item active" data-interval="10000">
                                    <img src="/images/tenancy/image.jpg" class="d-block w-100" alt="...">
                                </div>
                                <div class="carousel-item" data-interval="2000">
                                    <img src="/images/tenancy/image.jpg" class="d-block w-100" alt="...">
                                </div>
                                <div class="carousel-item" data-interval="2000">
                                    <img src="/images/tenancy/image.jpg" class="d-block w-100" alt="...">
                                </div>
                                <div class="carousel-item" data-interval="2000">
                                    <img src="/images/tenancy/image.jpg" class="d-block w-100" alt="...">
                                </div>
                                <div class="carousel-item" data-interval="2000">
                                    <img src="/images/tenancy/image.jpg" class="d-block w-100" alt="...">
                                </div>
                                <div class="carousel-item" data-interval="2000">
                                    <img src="/images/tenancy/image.jpg" class="d-block w-100" alt="...">
                                </div>
                                <div class="carousel-item">
                                    <img src="/images/tenancy/image.jpg" class="d-block w-100" alt="...">
                                </div>
                                <div class="carousel-item" data-interval="2000">
                                    <img src="/images/tenancy/image.jpg" class="d-block w-100" alt="...">
                                </div>
                            </div>
                            <a class="carousel-control-prev" href="#carouselExampleInterval" role="button" data-slide="prev">
                                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                <span class="sr-only">Previous</span>
                            </a>
                            <a class="carousel-control-next" href="#carouselExampleInterval" role="button" data-slide="next">
                                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                <span class="sr-only">Next</span>
                            </a>
                        </div>
                    </div>

                   
                </div>
        <div class="row pb-3 backgroundWhite">
   @foreach (var properties in Model)
        {
        <div class="col-lg-3 col-md-6">
            <div class="row p-2">
                <div class="col-12  p-1" style="border:1px solid #008cba; border-radius: 5px;">
                    <div class="card" style="border:0px;">
                        <img src="@properties.imagePath" class="card-img-top rounded" />
                        <div class="pl-1">
                            <p class="card-title h5"><b style="color:#2c3e50">@properties.propertyAddress</b></p>
                            @*<p class="card-title text-primary">by <b>@product.Artist</b></p>*@
                        </div>
                        <div style="padding-left:5px;">
                            <p>List Price: <b class="">$@properties.marketPrice.ToString("0.00")</b></p>
                        </div>
                        <div style="padding-left:5px;">
                            <p style="color:maroon">As low as: <b class="">@properties.estateDescription</b></p>
                        </div>
                    </div>
                    <div>
                        <a asp-action="Details" class="btn btn-primary form-control" asp-route-id="@properties.ID">Details</a>
                    </div>
                </div>
            </div>
        </div>
        }
        
                </div>
      
    <footer class="footer mt-auto py-3 bg-light">
  <div class="container">
    <  &copy; 2021 - BoldRealties.Web - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
  </div>
</footer>
       
      
   
