<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - BoldRealties.Web</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.css" />
    <link rel="stylesheet" href="~/BoldRealties.Web.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.7.2/font/bootstrap-icons.css">
</head>
<body>
    <partial name="Partial/Messages" />

         <div class="sidenav">
           <a  class="nav-link text-dark" asp-controller="PropertiesRS" asp-action="Index">Properties</a>
           <a class="nav-link text-dark" asp-controller="User" asp-action="Index">Users</a>
           <a class="nav-link text-dark" asp-controller="Tenancy" asp-action="Index">Tenancies</a>
           <a class="nav-link text-dark" asp-controller="Viewings" asp-action="Index">Viewings</a>
           <a class="nav-link text-dark" asp-controller="Reports" asp-action="Index">Reports</a>
           <a class="nav-link text-dark" asp-controller="User" asp-action="Index">Tenants</a>
           <a class="nav-link text-dark" asp-controller="User" asp-action="Index">Landlords</a>
           <a class="nav-link text-dark" asp-controller="User" asp-action="Index">Subcontractors</a>
           <a class="nav-link text-dark" asp-controller="Invoices" asp-action="Index">Invoices</a>
           <a class="nav-link text-dark" asp-controller="maintenanceJobs" asp-action="Index">Maintenance Jobs</a>
           <a class="nav-link text-dark" asp-controller="Enquiries" asp-action="Index">Enquiries & Applicants</a>
       </div>

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - BoldRealties.Web - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
     <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
